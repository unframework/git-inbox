{
    "name": "Git Inbox: The Bot",
    "description": "git-inbox: Slack bot to convert uploads into Git commits/PRs",
    "repository": "https://github.com/unframework/git-inbox",
    "keywords": [
        "slack",
        "bot",
        "git",
        "github",
        "automation",
        "yaml",
        "xls",
        "xlsx",
        "excel"
    ],
    "env": {
        "SLACK_AUTH_TOKEN": {
            "description": "Slack auth token"
        },
        "TARGET_GIT_URL": {
            "description": "Git repo to work on: https://<user>:<personal-token>@github.com/<user>/<repo>.git"
        }
    }
}
